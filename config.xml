<?xml version='1.0' encoding='utf-8'?>
<widget id="com.health.safe.care.pet" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pet Safe Care</name>
    <description>
        This is for educational underdegree
    </description>
    <author email="lucasgomes78@gmail.com" href="">
        Lucas Eduardo Gomes
    </author>
    <preference name="StatusBarBackgroundColor" value="#1976d2" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="/res/icon/android/ldpi_icon.png" />
        <icon density="mdpi" src="/res/icon/android/mdpi_icon.png" />
        <icon density="hdpi" src="/res/icon/android/hdpi_icon.png" />
        <icon density="xhdpi" src="/res/icon/android/xhdpi_icon.png" />
        <icon density="xxhdpi" src="/res/icon/android/xxhdpi_icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <plugin name="cordova-plugin-call-number" spec="^1.0.1" />
    <engine name="android" spec="^7.1.1" />
    <engine name="browser" spec="^5.0.4" />
</widget>
